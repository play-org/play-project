{
  "presets": [
    [
      "@babel/env",
      {
        "loose": true,
        "corejs": "3.2.0",
        "targets": {
          "browsers": ["ie >=9", "last 2 version", "> 5%", "not dead"]
        },
        "useBuiltIns": "usage"
      }
    ],
    "@babel/react",
    "@babel/typescript"
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"]
}
